{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.1.*",
		"silverstripe/framework": "3.1.*",
		"silverstripe-themes/simple": "3.1.*",
		"silverstripe/userforms": "dev-feature/multi-page-forms as 2.1"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7"
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"repositories": [{
		"type": "vcs",
		"url": "https://github.com/open-sausages/silverstripe-userforms"
	}]
}
