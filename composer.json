{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "dev-integrate-react as 4.0.x-dev",
		"silverstripe/framework": "dev-integrate-react as 4.0.x-dev",
		"silverstripe/asset-admin": "dev-master",
		"silverstripe/reports": "dev-master",
		"silverstripe/siteconfig": "dev-master",
		"silverstripe-themes/simple": "3.1.*"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "4.0.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"repositories": [{
		"type": "vcs",
		"url": "https://github.com/open-sausages/silverstripe-framework"
	},{
		"type": "vcs",
		"url": "https://github.com/open-sausages/silverstripe-cms"
	},{
		"type": "vcs",
		"url": "https://github.com/open-sausages/silverstripe-asset-admin"
	}]
}
