{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.2",
		"scott1702/cms": "dev-integrate-asset-gallery-view",
		"open-sausages/framework": "dev-dbfile-dataobject-gallery-view",
		"silverstripe-themes/simple": "*",
		"open-sausages/asset-gallery-field": "dev-master"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"config": {
		"process-timeout": 2000
	},
	"minimum-stability": "dev",
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:open-sausages/silverstripe-framework.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:scott1702/silverstripe-cms.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:open-sausages/silverstripe-assets-gallery.git"
		}
	]
}
