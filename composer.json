{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.2",
		"silverstripe/cms": "~3.1.0@stable",
		"silverstripe/framework": "~3.1.0@stable",
		"silverstripe-themes/simple": "*",
		"silverstripe/userforms": "dev-demo-y"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"config": {
		"process-timeout": 600
	},
	"minimum-stability": "dev",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/open-sausages/silverstripe-userforms.git"
		}
	]
}
	