{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.2",
		"silverstripe/cms": "3.1.x-dev",
		"silverstripe/framework": "3.1.x-dev",
		"silverstripe-themes/simple": "*",
		
		"silverstripe-australia/gridfieldextensions": "1.1.0",
		"silverstripe/segment-field": "^1.0",
		"silverstripe/userforms": "dev-feature/multi-page-forms-v2"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"config": {
		"process-timeout": 600
	},
	"minimum-stability": "dev",
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:open-sausages/silverstripe-userforms.git"
		}
	]
}
