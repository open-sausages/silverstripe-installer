{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"repositories": [{
		"type": "vcs",
		"url": "https://github.com/open-sausages/silverstripe-framework"
	},{
		"type": "vcs",
		"url": "https://github.com/open-sausages/silverstripe-cms"
	},{
		"type": "vcs",
		"url": "https://github.com/open-sausages/silverstripe-assets-gallery"
	},{
		"type": "vcs",
		"url": "https://github.com/open-sausages/silverstripe-reactjs-common"
	}],
	"require": {
		"php": ">=5.3.2",
		"silverstripe/cms": "dev-integrate-asset-gallery-view as 4.0.x-dev",
		"silverstripe/framework": "dev-insert-media-gallery as 4.0.x-dev",
		"silverstripe/asset-gallery-field": "dev-master",
		"silverstripe/reactjs-common": "dev-master",
		"silverstripe-themes/simple": "*"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"config": {
		"process-timeout": 600
	},
	"minimum-stability": "dev"
}
